package br.com.bytebank.banco.test;


public class Teste {
	
	//Array[]
	public static void main(String[] args) {
		
		int[] idade = new int[5];//inicia a array com 0


		for(int i = 0; i < idade.length; i++) {
            idade[i] = i * i;
        }

        for(int i = 0; i < idade.length; i++) {
            System.out.println(idade[i]);
        }


	}
	
	
	}
	

	
	
	

